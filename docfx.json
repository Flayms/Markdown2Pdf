{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**/Markdown2Pdf/Markdown2Pdf.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "../Markdown2Pdf.wiki",
      "outputFormat": "markdown"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**",
          "**/node_modules/**",
          "**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "assets/**"
        ]
      }
    ],
    "output": "../Markdown2Pdf.wiki/_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Markdown2Pdf",
      "_appTitle": "Markdown2Pdf",
      "_enableSearch": true,
      "pdf": false
    }
  }
}